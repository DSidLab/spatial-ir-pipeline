nextflow_pipeline {

    name "Test Workflow main.nf"
    script "main.nf"

    test("RCTD on Visium SD with reference as URL") {

        tag "basic-sd"

        when {
            params {
                input = "../../../tests/test.csv"
                outdir = "tests/results"
                max_memory = "8.GB"
                max_cpus = 4
            }
        }

        then {
            assert workflow.success
        }

    }

}
